{
    "name": "Guinea Pig Youtube Progression Bar",
    "description": "An awesome progression bar",
    "version": "1.2",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],

        "css": ["main.css"],
        "js": ["content.js"]
    }],
    "web_accessible_resources": ["main.css", "images/*.*"],
    "icons": {
        "128": "images/icon128.png"
    }
}